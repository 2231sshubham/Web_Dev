<%- include('header.ejs'); -%>

  <div class="box" id="heading">
    <h1><%= listTitle %></h1>
  </div>

  <div class="box" id="list">
    <% for (var i = 0; i < item.length; i++) { %>
      <% if(item[i]!="") { %>
        <div class="item">
          <input type="checkbox">
          <p> <%= item[i] %> </p>
        </div>
      <% } else { continue; }%>
    <% } %>

    <form class="item" action="/" method="post">
      <input type="text" name="nt" placeholder="New item" autocomplete="off">
      <button type="submit" name="ls" value=<%= listTitle%>>+</button>
    </form>

  </div>
<%- include('footer.ejs') -%>
